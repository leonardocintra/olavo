{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "Item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "descricao": {
      "type": "string",
      "required": true,
      "maxLength": 50,
      "minLength": 2
    },
    "preco": {
      "type": "decimal",
      "default": 0,
      "required": true
    },
    "item_tipo_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::item-tipo.item-tipo"
    }
  }
}
